<?xml version='1.0' encoding='utf-8'?>
<widget id="es.patbook.app.lite" version="1.0.134" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Patbook Lite</name>
    <description/>
    <author email="info@pat-book.com" href="http://www.pat-book.com">El Artesano de Ideas</author>
    <content src="index.html"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <allow-navigation href="data:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
        <icon density="ldpi" src="res/icon/android/drawable-ldpi-icon.png"/>
        <icon density="mdpi" src="res/icon/android/drawable-mdpi-icon.png"/>
        <icon density="hdpi" src="res/icon/android/drawable-hdpi-icon.png"/>
        <icon density="xhdpi" src="res/icon/android/drawable-xhdpi-icon.png"/>
    </platform>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="StatusBarOverlaysWebView" value="true"/>
    <preference name="Fullscreen" value="false"/>
    <preference name="StatusBarStyle" value="lightcontent"/>
    <preference name="phonegap-version" value="cli-9.0.0"/>
    <preference name="android-minSdkVersion" value="19"/>
    <preference name="android-targetSdkVersion" value="28"/>
    <plugin name="cordova-plugin-statusbar"/>
    <plugin name="cordova-plugin-camera" spec="~2.4.1"/>
    <plugin name="cordova-plugin-file" spec="~4.3.3"/>
    <plugin name="cordova-plugin-file-transfer" spec="~1.5.0"/>
    <plugin name="com-sarriaroman-photoviewer" source="npm" spec="~1.2.1"/>
    <plugin name="cordova-plugin-device"/>
    <plugin name="cordova-admob-plus" source="npm">
        <variable name="APP_ID_ANDROID" value="ca-app-pub-3279001848353929~5264715374"/>
    </plugin>
    <engine name="android" spec="8.0.0"/>
    <engine name="ios" spec="^4.1.1"/>
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3"/>
</widget>
